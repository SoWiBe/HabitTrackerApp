<Window x:Class="HabitTrackerApp.UI.Views.Popups.CreateHabitPopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:HabitTrackerApp.UI.Views.Popups"
        xmlns:viewModels="clr-namespace:HabitTrackerApp.ViewModels"
        mc:Ignorable="d"
        Title="CreateHabitPopup" Height="600" Width="300">
    
    <Window.DataContext>
        <viewModels:CreateHabitViewModel/>
    </Window.DataContext>
    
    <Border
        Height="600"
        Width="300"
        Style="{DynamicResource CreateHabitBorder}"
        Background="{DynamicResource MainBackgroundColor}">
        <Grid>
            <TextBlock>Some</TextBlock>
        </Grid>
    </Border>
</Window>
